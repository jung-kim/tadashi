{{#if _authToken }}
<button class="btn disabled btn-outline-info my-2 my-sm-0" type="button">
    <img src="{{_user.profile_image_url}}" />
    {{_user.login}}
</button>
<button class="btn btn-outline-warning my-2 my-sm-0" onclick="authLogout()" type="button">
    Logout
</button>
{{else}}
<button class="btn btn-outline-success my-2 my-sm-0" onclick="authenticate()" type="button">
    Login / Sign up
</button>
{{/if}}