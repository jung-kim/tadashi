    "http-status-codes": "^2.1.4",
    "madge": "^5.0.1",
    "material-color-hash": "^0.1.6",
    "md5": "^2.3.0",
    "micro-signals": "^2.4.0",
    "p-limit": "^1.3.0",
    "pako": "^2.0.3",
    "tmi.js": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "aws-sdk": "^2.842.0",
    "babelify": "^10.0.0",
    "chai": "^4.3.4",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "esm": "3.2.25",
    "exposify": "^0.5.0",
    "fetch-mock": "^9.11.0",
    "glob": "^7.1.7",
    "htmlmin": "0.0.7",
    "istanbul": "^0.4.5",
    "less": "^3.12.2",
