    "mocha-multi-reporters": "^1.5.1",
    "moment-timezone": "^0.5.32",
    "node-fetch": "^2.6.2",
    "node-localstorage": "^2.1.6",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2"
  }
