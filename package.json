{
  "name": "twitch-dash",
  "version": "0.0.1",
  "description": "",
  "main": "",
  "scripts": {
    "prep": "npm run env-gen && awk '{gsub(/\"es2015\"/,\"\\\"@babel/preset-env\\\"\")}1' ./node_modules/tmi.js/package.json > temp.txt && mv temp.txt ./node_modules/tmi.js/package.json",
    "env-gen": "node ./scripts/env_export.js",
    "test": "npm run prep && ./node_modules/mocha/bin/mocha --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi-config.json --timeout 5000 --exit --file ./tests/testSetup.js './tests/**/*'",
    "cov": "npm run coverage",
    "coverage": "npm run prep && ./node_modules/nyc/bin/nyc.js --reporter=json --reporter=text --reporter=html ./node_modules/mocha/bin/mocha --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi-config.json --timeout 5000 --exit --file ./tests/testSetup.js './tests/**/*'",
    "build": "npm run prep && node ./scripts/build.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js './js/**/*.js' './tests/**/*.js' './scripts/**/*.js' './proxy-server/**/*.js' --global Twitch --global moment"
  },
  "author": "jung-kim.github.io",
  "license": "MIT",
  "dependencies": {
    "@ladjs/env": "^3.0.0",
    "browserify": "^17.0.0",
